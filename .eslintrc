{
    "extends": [
        "eslint:recommended", 
        "plugin:react/recommended"
    ],
    "settings": {
        "react": {
            "version": "latest"
        }
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2018,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jasmine": true
    },
    "rules": {
        "no-console": "off",
        "semi": "warn"
    }
}